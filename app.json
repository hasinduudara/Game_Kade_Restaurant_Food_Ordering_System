{
  "expo": {
    "name": "Game Kade",
    "slug": "game-kade",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gamekade",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D93800"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "statusBar": {
        "style": "dark",
        "backgroundColor": "#ffffff",
        "translucent": false
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBw1nfBp8QT_DAEkG5_UC3O-vUiju4yguk"
        }
      },
      "navigationBar": {
        "backgroundColor": "#ffffff",
        "visible": "always"
      },
      "package": "com.hasinduudara.gamekade",
      "adaptiveIcon": {
        "backgroundColor": "#D93800",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#D93800",
          "dark": {
            "backgroundColor": "#D93800"
          }
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "Sorry, we need camera roll permissions!"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow location access to find you."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aee16bfa-bd04-4efe-a281-a8062a2252b1"
      }
    }
  }
}
